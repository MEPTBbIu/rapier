{
  "name": "rapier",
  "version": "1.0.1",
  "description": "A JavaScript Dota 2 (Source 2) replay parsing library.",
  "main": "Parser.js",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "async": "^1.4.2",
    "bytebuffer": "^4.0.0",
    "long": "^2.2.5",
    "protobufjs": "4.0.0"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "json-loader": "^0.5.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
